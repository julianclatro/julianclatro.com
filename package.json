    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.6.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.7",
